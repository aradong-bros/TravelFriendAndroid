<?xml version="1.0" encoding="UTF-8" ?> <!-- MyBatis 설정 파일 -->
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- ~~.xml 등  매핑파일들의 resultType 이름에 아리아스를 주어 간단히 만든다. -->
	<typeAliases>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.AttractionVo" alias="attractionvo"/> <!-- 아리아스는 모두 소문자로 쓰는게 관례. 대문자로 써도 상관은 없다. -->
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.CityListVo" alias="citylistvo"/> <!-- 아리아스는 모두 소문자로 쓰는게 관례. 대문자로 써도 상관은 없다. -->
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.UserVo" alias="uservo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.TrainCategoryVo" alias="traincategoryvo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.TrainInfoVo" alias="traininfovo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.TrainLineVo" alias="trainlinevo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.TrainOperationRouteVo" alias="trainoperationroutevo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.TrainStationVo" alias="trainstationvo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.CommentVo" alias="commentvo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.ScheduleVo" alias="schedulevo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.CityVo" alias="cityvo"/>
		<typeAlias type="com.estsoft.futures.aradongbros.travelfriend.vo.PostVo" alias="postvo"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/mappers/android.xml" />
		<mapper resource="mybatis/mappers/user.xml"/>
		<mapper resource="mybatis/mappers/train.xml"/>
		<mapper resource="mybatis/mappers/comment.xml"/>
		<mapper resource="mybatis/mappers/schedule.xml"/>
		<mapper resource="mybatis/mappers/city.xml"/>
		<mapper resource="mybatis/mappers/post.xml"/>
	</mappers>
</configuration>